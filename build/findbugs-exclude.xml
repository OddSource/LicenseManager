<FindBugsFilter>
    <Match>
        <Class name="ImmutableArrayList" />
        <Bug pattern="BC_BAD_CAST_TO_CONCRETE_COLLECTION" />
    </Match>

    <Match>
        <Class name="ImmutableLinkedHashSet" />
        <Bug pattern="BC_BAD_CAST_TO_CONCRETE_COLLECTION" />
    </Match>

    <Match>
        <Or>
            <Class name="InsecureEnvironmentException" />
            <Class name="ImmutableModifiedThroughReflectionException" />
        </Or>
        <Bug pattern="NM_CLASS_NOT_EXCEPTION" />
    </Match>

    <Match>
        <Class name="ObjectSerializer" />
        <Method name="readObject" />
        <Bug pattern="DE_MIGHT_IGNORE" />
    </Match>
</FindBugsFilter>